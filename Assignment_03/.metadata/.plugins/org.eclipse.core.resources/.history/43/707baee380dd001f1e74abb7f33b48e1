import java.util.Map;
import java.util.Set;

public class MainService {
	public static void main(String[] args) {
		try {
			ImageFetcher imageFetcher = new ImageFetcher();
			BlogData blogData = imageFetcher.fetchBlogData("good",1,5);
			Map<Integer,Set<String>> postNoWithImageUrls = blogData.postNoWithImageUrls;
			for(Integer postNo : postNoWithImageUrls.keySet()) {
				System.out.print(postNo + ". ");
				for(String imageUrl : postNoWithImageUrls.get(postNo)) {
					System.out.println(imageUrl);
				}
			}
		}
		catch(Exception exception) {
			System.out.println(exception.getMessage());
		}
	}
}

